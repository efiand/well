    "gh-pages": "^1.2.0",
    "gulp": "3.9.x",
    "gulp-concat": "^2.6.1",
    "gulp-csscomb": "^3.0.8",
    "gulp-csso": "^3.0.1",
    "gulp-htmlmin": "^4.0.0",
    "gulp-imagemin": "^4.1.0",
    "gulp-nunjucks": "^4.0.0",
    "gulp-plumber": "1.2.x",
    "gulp-postcss": "7.0.x",
    "gulp-rename": "^1.2.2",
    "gulp-replace-task": "^0.11.0",
    "gulp-sass": "4.0.x",
    "gulp-svg-sprites": "^4.1.2",
    "gulp-svgstore": "^6.1.1",
    "gulp-uglify": "^3.0.0",
    "gulp-webp": "^3.0.0",
    "gulp.spritesmith": "^6.9.0",
    "imagemin-jpegoptim": "^5.2.0",
    "picturefill": "^3.0.2",
    "run-sequence": "^2.2.1",
    "stylelint": "9.2.x",
    "stylelint-config-htmlacademy": "0.1.x",
    "svg4everybody": "^2.1.9"
  },
  "scripts": {
    "editorconfig": "editorconfig-cli",
    "eslint": "eslint js/",
    "stylelint": "stylelint source/sass/**/*.scss --syntax scss",
    "test": "npm run editorconfig && npm run stylelint && npm run eslint",
    "prepare": "gulp build:dev",
    "start": "npm run prepare && gulp serve",
    "build": "gulp build",
    "deploy": "npm run build && gh-pages -d build && gulp build:del"
  },
  "editorconfig-cli": [
    "*.json",
    "*.js",
    "source/*.html",
    "source/js/**/*.js",
    "source/img/**/*.svg",
    "source/sass/**/*.{sass,scss}"
  ],
  "engines": {
    "node": "8.11"
  },
  "dependencies": {}
}
